{
  "name": "@wellwright/traverse",
  "version": "0.1.2",
  "description": "Performance data capture and analysis toolkit for web applications",
  "type": "module",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "traverse": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup && cp ../../README.md ./README.md",
    "dev": "bun run src/cli/index.ts",
    "test": "bun test",
    "typecheck": "bun run tsc --noEmit",
    "prepublishOnly": "bun run typecheck && bun test && bun run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devdumpling/traverse.git"
  },
  "keywords": [
    "performance",
    "web-vitals",
    "playwright",
    "benchmark",
    "bundle-analysis"
  ],
  "dependencies": {
    "fast-glob": "^3.3.3",
    "playwright": "^1.57.0"
  },
  "devDependencies": {
    "@types/bun": "^1.3.6",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}